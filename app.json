{
  "expo": {
    "name": "HallMate",
    "slug": "class-book-fe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/hall-mate-splash-design.webp",
      "resizeMode": "cover",
      "height": "100%",
      "width": "100%"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hallmate"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hallmate",
      "permissions": ["android.permission.RECORD_AUDIO", "INTERNET"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "This app wants to access your camera"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b36050ed-26e9-4c43-af0e-c3c38cd029b8"
      }
    },
    "owner": "auspicious14"
  }
}
